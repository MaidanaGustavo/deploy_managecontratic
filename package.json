{
  "name": "contratic",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:backend": "next dev -p 3001",
    "dev:all": "npm run dev:backend & npm run dev",
    "build": "vite build && next build",
    "build:production": "npm install --omit=dev && npm run build:frontend",
    "build:frontend": "vite build",
    "build:backend": "next build",
    "start": "serve -s public -l $PORT",
    "start:backend": "next start -p 3001",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:models": "vitest run models/__tests__",
    "seed": "node scripts/seed.js",
    "db:reset": "node scripts/seed.js"
  },
  "dependencies": {
    "@tiptap/extension-color": "^3.9.0",
    "@tiptap/extension-font-family": "^3.9.0",
    "@tiptap/extension-highlight": "^3.9.0",
    "@tiptap/extension-link": "^3.9.0",
    "@tiptap/extension-table": "^3.9.0",
    "@tiptap/extension-table-cell": "^3.9.0",
    "@tiptap/extension-table-header": "^3.9.0",
    "@tiptap/extension-table-row": "^3.9.0",
    "@tiptap/extension-text-align": "^3.9.0",
    "@tiptap/extension-text-style": "^3.9.0",
    "@tiptap/extension-underline": "^3.9.0",
    "@tiptap/react": "^3.8.0",
    "@tiptap/starter-kit": "^3.8.0",
    "bcryptjs": "^3.0.2",
    "lucide-react": "^0.548.0",
    "mongoose": "^8.19.2",
    "next": "^16.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "serve": "^14.2.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
